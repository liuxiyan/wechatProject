@import "../../templates/num-cell.wxss";

page{
  background-color: #fff;
  padding: 120rpx 30rpx 0;
}

.container{
  width: 100%;
  background-color: #f9f9f9;
}

.header-top{
  position: fixed;
  background: url("data:image/png;base64,") no-repeat;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  height: 100rpx;
  border-bottom: 1px solid #eee;
  z-index: 99;
  background-color: #fafafa;
}

.header-top-question,.header-top-new{
  flex: 1;
  text-align: center;
  line-height: 100rpx;
  color: #363636;
  font-size: 15px;
}

.header-top .current{
  border-bottom: 6rpx solid #15aafa;
  color: #6ec3fc;
}

/*底部*/
.bottom{
  height: 130rpx;
  position: relative;
  background-color: #f9f9f9;
  overflow: hidden;
  text-align: center;
}

.bottom .bottom-line{
  position: absolute;
  height: 2rpx;
  width: 100%;
  top: 64rpx;
  left: 0;
  background-color: #f4f4f4;
  z-index: 0;
}

.bottom .bottom-tip{
  position: relative;
  display: inline-block;
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #a9a9a9;
  line-height: 130rpx;
  background-color: #f9f9f9;
  z-index: 1;
}

